%table.table
  %thead
    %tr
      %th
        ID
      %th
        Date
      %th
        Customer
      %th
        Actions
  %tbody
    - @delivery_notes.each do |note|
      %tr
        %td
          = note.id
        %td
          = note.date
        %td
          = note.customer
        %td
          = button_to 'Show', delivery_note_path(note), method: :get
= link_to 'New delivery note', new_delivery_note_path
|
= link_to 'Back', root_path
